{
  "$schema": "https://mintlify.com/schema.json",
  "name": "WalletLabels API",
  "logo": {
    "dark": "/logo/logo.png",
    "light": "/logo/logo.png"
  },
  "api": {
    "baseUrl": "https://api-c.walletlabels.xyz/",
    "auth": {
      "method": "key",
      "name": "x-api-key",
      "inputPrefix": "demo-api-key-x-00000"
    },
    "playground": {
      "mode": "show"
    }
  },

  "favicon": "/favicon.png",
  "colors": {
    "primary": "#9563FF",
    "light": "#AE87FF",
    "dark": "#0D001D",
    "background": {
      "dark": "#090014"
    },
    "anchors": {
      "from": "#FF7F57",
      "to": "#9563FF"
    }
  },
  "modeToggle": {
    "default": "light",
    "isHidden": true
  },
  "topbarCtaButton": {
    "name": "Get an API key",
    "url": "https://WalletLabels.xyz"
  },
  "footerSocials": {
    "twitter": "https://twitter.com/walletlabels",
    "telegram": "https://t.me/+yDF9bnv2R7RkNWZk"
  },

  "anchors": [
    {
      "name": "API Reference",
      "icon": "code",
      "url": "content/"
    },

    {
      "name": "Community",
      "icon": "telegram",
      "url": "https://t.me/+yDF9bnv2R7RkNWZk"
    }
  ],
  "navigation": [
    {
      "group": "Getting Started",
      "pages": ["content/api-references"]
    },
    {
      "group": "Essentials",
      "pages": [
        {
          "group": "Ethereum API",
          "pages": [
            "endpoint/search",
            "endpoint/label",
            "endpoint/mev_label",
            "endpoint/mev_search"
          ]
        },
        {
          "group": "Optimism API",
          "pages": ["endpoint/optimism/label"]
        },
        {
          "group": "Arbitrum API",
          "pages": ["endpoint/arbitrum/label"]
        },
        {
          "group": "Solana API",
          "pages": ["endpoint/solana/label", "endpoint/solana/search"]
        }
      ]
    }
  ]
}
